# AI-EOS Configuration
# Development Environment

# Database (Optional for now)
DATABASE_URL=postgresql://user:password@localhost:5432/ai_eos
REDIS_URL=redis://localhost:6379/0

# LLM APIs (REQUIRED - Add your keys here)
GEMINI_API_KEY=your_gemini_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# LangSmith Observability (CRITICAL for debugging deep agents)
LANGCHAIN_TRACING_V2=false
LANGCHAIN_API_KEY=your_langsmith_api_key_here
LANGCHAIN_PROJECT=ai-eos-persian-advisor
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com

# Application
ENVIRONMENT=development
LOG_LEVEL=INFO

# Feature Flags
ENABLE_MULTI_AGENT=true
ENABLE_RL_TRAINING=false
ENABLE_VOICE=false

